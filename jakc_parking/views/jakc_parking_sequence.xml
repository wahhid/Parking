<?xml version="1.0" encoding="windows-1252"?>
<openerp>
    <data>
 		<!-- PARKING/SEQUENCE -->

        <!-- TRANSACTION ID -->
        <record id="sequence_parking_transaction" model="ir.sequence">
            <field name="name">Parking Transaction</field>
            <field name="code">parking.transaction.sequence</field>			           
            <field name="padding" eval="5"/>
            <field name="number_increment">1</field>            
        </record>
       
        <!-- MEMBERSHIP ID -->
        <record id="sequence_parking_membership" model="ir.sequence">
            <field name="name">Parking Membership</field>
            <field name="code">parking.membership.sequence</field>			           
            <field name="padding" eval="10"/>
            <field name="number_increment">1</field>            
        </record>
       
	</data>
</openerp>